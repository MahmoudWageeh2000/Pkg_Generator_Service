<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ern:NewReleaseMessage xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xmlns:ern="http://ddex.net/xml/ern/34" MessageSchemaVersionId="ern/34" LanguageAndScriptCode="en" xs:schemaLocation="http://ddex.net/xml/ern/34/release-notification.xsd">
  <MessageHeader>
    <MessageThreadId>@[ALBUM_UPC]@</MessageThreadId>
    <MessageId>@[ALBUM_UPC]@</MessageId>
    <MessageSender>
      <PartyId>PADPIDA20131118019</PartyId>
      <PartyName>
        <FullName>Mazzika Group</FullName>
      </PartyName>
    </MessageSender>
    <MessageRecipient>
      <PartyId>PADPIDA2013020802I</PartyId>
      <PartyName>
        <FullName>YouTube</FullName>
      </PartyName>
    </MessageRecipient>
    <MessageCreatedDateTime>@[DATE_TIME]@</MessageCreatedDateTime>
    <MessageControlType>LiveMessage</MessageControlType>
  </MessageHeader>
  <UpdateIndicator xmlns="">UpdateMessage</UpdateIndicator>
  <ResourceList>
		#LOOP_SOUND_RECORDING#
    <SoundRecording>
      <SoundRecordingType>MusicalWorkSoundRecording</SoundRecordingType>
      <SoundRecordingId>
        <ISRC>@[ISRC]@</ISRC>
        <ProprietaryId Namespace="DPID:PADPIDA20131118019">GGL_USR_ID:@[ISRC]@</ProprietaryId>
      </SoundRecordingId>
      <ResourceReference>A@[TRACK_NO]@</ResourceReference>
      <ReferenceTitle>
        <TitleText>@[TRACK_TITLE]@</TitleText>
      </ReferenceTitle>
      <Duration>@[TRACK_DURATION]@</Duration>
      <SoundRecordingDetailsByTerritory>
        <TerritoryCode>@[TERRITORY_CODE]@</TerritoryCode>
        <Title TitleType="DisplayTitle">
          <TitleText>@[TRACK_TITLE]@</TitleText>
        </Title>
        <DisplayArtist>
          <PartyName>
            <FullName>@[TRACK_ARTIST]@</FullName>
          </PartyName>
          <ArtistRole>MainArtist</ArtistRole>
        </DisplayArtist>
		    #LOOP_TRACK_FEATURED_ARTIST#
				<DisplayArtist>
					<PartyName>
						<FullName>@[TRACK_FEATURED_ARTIST]@</FullName>
					</PartyName>
					<ArtistRole>FeaturedArtist</ArtistRole>
				</DisplayArtist>
				#END_LOOP_TRACK_FEATURED_ARTIST#
        <LabelName>@[LABEL_NAME]@</LabelName>
        <PLine>
          <Year>@[RELEASE_YEAR]@</Year>
          <PLineText>@[RELEASE_YEAR]@ @[LABEL_NAME]@</PLineText>
        </PLine>
        <Genre>
          <GenreText>@[TRACK_GENRE]@</GenreText>
          <SubGenre>@[TRACK_GENRE]@</SubGenre>
        </Genre>
        <ParentalWarningType>NotExplicit</ParentalWarningType>
      </SoundRecordingDetailsByTerritory>
    </SoundRecording>
		#END_LOOP_SOUND_RECORDING#
    <Image>
      <ImageType>FrontCoverImage</ImageType>
      <ImageId>
        <ProprietaryId Namespace="DPID:PADPIDA20131118019">GGL_UIM_ID:@[ALBUM_UPC]@.jpg</ProprietaryId>
      </ImageId>
      <ResourceReference>A@[IMAGE_NO]@</ResourceReference>
      <ImageDetailsByTerritory>
        <TerritoryCode>@[TERRITORY_CODE]@</TerritoryCode>
        <TechnicalImageDetails>
          <TechnicalResourceDetailsReference>T@[IMAGE_NO]@</TechnicalResourceDetailsReference>
          <ImageCodecType>JPEG</ImageCodecType>
          <ImageHeight>@[IMAGE_HEIGHT]@</ImageHeight>
          <ImageWidth>@[IMAGE_WIDTH]@</ImageWidth>
          <File>
            <FileName>@[ALBUM_UPC]@.jpg</FileName>
          </File>
        </TechnicalImageDetails>
      </ImageDetailsByTerritory>
    </Image>
  </ResourceList>
  <ReleaseList>
    <Release>
      <ReleaseId>
        <ICPN IsEan="false">@[ALBUM_UPC]@</ICPN>
        <ProprietaryId Namespace="DPID:PADPIDA20131118019">GGL_UR_ID:@[ALBUM_UPC]@</ProprietaryId>
      </ReleaseId>
      <ReleaseReference>R0</ReleaseReference>
      <ReferenceTitle>
        <TitleText>@[ALBUM_TITLE]@</TitleText>
      </ReferenceTitle>
      <ReleaseResourceReferenceList>
				#LOOP_RELEASE_RESOURCE_REFERENCE_LIST#
        <ReleaseResourceReference ReleaseResourceType="PrimaryResource">A@[TRACK_NO]@</ReleaseResourceReference>
				#END_LOOP_RELEASE_RESOURCE_REFERENCE_LIST#
        <ReleaseResourceReference ReleaseResourceType="SecondaryResource">A@[IMAGE_NO]@</ReleaseResourceReference>
      </ReleaseResourceReferenceList>
      <ReleaseType>@[RELEASE_TYPE]@</ReleaseType>
      <ReleaseDetailsByTerritory>
        <TerritoryCode>@[TERRITORY_CODE]@</TerritoryCode>
        <LabelName>@[LABEL_NAME]@</LabelName>
        <Title TitleType="DisplayTitle">
          <TitleText>@[ALBUM_TITLE]@</TitleText>
        </Title>
        <DisplayArtist>
          <PartyName>
            <FullName>@[ALBUM_ARTIST]@</FullName>
          </PartyName>
          <ArtistRole>MainArtist</ArtistRole>
        </DisplayArtist>
		    #LOOP_ALBUM_FEATURED_ARTIST#
				<DisplayArtist>
					<PartyName>
						<FullName>@[ALBUM_FEATURED_ARTIST]@</FullName>
					</PartyName>
					<ArtistRole>FeaturedArtist</ArtistRole>
				</DisplayArtist>
				#END_LOOP_ALBUM_FEATURED_ARTIST#
        <ParentalWarningType>NotExplicit</ParentalWarningType>
        <Genre>
          <GenreText>@[TRACK_GENRE]@</GenreText>
          <SubGenre>@[TRACK_GENRE]@</SubGenre>
        </Genre>
        <OriginalReleaseDate>@[RELEASE_DATE]@</OriginalReleaseDate>
      </ReleaseDetailsByTerritory>
      <PLine>
        <Year>@[RELEASE_YEAR]@</Year>
        <PLineText>@[RELEASE_YEAR]@ @[LABEL_NAME]@</PLineText>
      </PLine>
      <CLine>
        <Year>@[RELEASE_YEAR]@</Year>
        <CLineText>@[RELEASE_YEAR]@ @[LABEL_NAME]@</CLineText>
      </CLine>
    </Release>
		#LOOP_RELEASE#
    <Release>
      <ReleaseId>
        <ISRC>@[ISRC]@</ISRC>
        <ProprietaryId Namespace="DPID:PADPIDA20131118019">@[ISRC]@</ProprietaryId>
      </ReleaseId>
      <ReleaseReference>R@[TRACK_NO]@</ReleaseReference>
      <ReferenceTitle>
        <TitleText>@[TRACK_TITLE]@</TitleText>
      </ReferenceTitle>
      <ReleaseResourceReferenceList>
        <ReleaseResourceReference ReleaseResourceType="PrimaryResource">A@[TRACK_NO]@</ReleaseResourceReference>
      </ReleaseResourceReferenceList>
      <ReleaseType>TrackRelease</ReleaseType>
      <ReleaseDetailsByTerritory>
        <TerritoryCode>@[TERRITORY_CODE]@</TerritoryCode>
        <DisplayArtistName>@[TRACK_ARTIST]@</DisplayArtistName>
        <LabelName>@[LABEL_NAME]@</LabelName>
        <Title TitleType="DisplayTitle">
          <TitleText>@[TRACK_TITLE]@</TitleText>
        </Title>
        <DisplayArtist>
          <PartyName>
            <FullName>@[TRACK_ARTIST]@</FullName>
          </PartyName>
          <ArtistRole>MainArtist</ArtistRole>
        </DisplayArtist>
		    #LOOP_TRACK_FEATURED_ARTIST#
				<DisplayArtist>
					<PartyName>
						<FullName>@[TRACK_FEATURED_ARTIST]@</FullName>
					</PartyName>
					<ArtistRole>FeaturedArtist</ArtistRole>
				</DisplayArtist>
				#END_LOOP_TRACK_FEATURED_ARTIST#
      </ReleaseDetailsByTerritory>
      <PLine>
        <Year>@[RELEASE_YEAR]@</Year>
        <PLineText>@[RELEASE_YEAR]@ @[LABEL_NAME]@</PLineText>
      </PLine>
    </Release>
		#END_LOOP_RELEASE#
  </ReleaseList>
  <DealList>
    <ReleaseDeal>
      <DealReleaseReference>R0</DealReleaseReference>
      <Deal>
        <DealTerms>
          <CommercialModelType>SubscriptionModel</CommercialModelType>
          <CommercialModelType>AdvertisementSupportedModel</CommercialModelType>
          <Usage>
            <UseType>OnDemandStream</UseType>
          </Usage>
          <TerritoryCode>@[TERRITORY_CODE]@</TerritoryCode>
          <ValidityPeriod>
            <StartDate>@[START_DATE]@</StartDate>
          </ValidityPeriod>
        </DealTerms>
      </Deal>
      <Deal>
        <DealTerms>
          <CommercialModelType>RightsClaimModel</CommercialModelType>
          <Usage>
            <UseType>UserMakeAvailableUserProvided</UseType>
          </Usage>
          <TerritoryCode>@[TERRITORY_CODE]@</TerritoryCode>
          <ValidityPeriod/>
          <RightsClaimPolicy>
            <RightsClaimPolicyType>Monetize</RightsClaimPolicyType>
          </RightsClaimPolicy>
        </DealTerms>
      </Deal>
      <EffectiveDate>@[START_DATE]@</EffectiveDate>
    </ReleaseDeal>
		#LOOP_RELEASE_DEAL#
    <ReleaseDeal>
      <DealReleaseReference>R@[TRACK_NO]@</DealReleaseReference>
      <Deal>
        <DealTerms>
          <CommercialModelType>SubscriptionModel</CommercialModelType>
          <CommercialModelType>AdvertisementSupportedModel</CommercialModelType>
          <Usage>
            <UseType>OnDemandStream</UseType>
          </Usage>
          <TerritoryCode>@[TERRITORY_CODE]@</TerritoryCode>
          <ValidityPeriod>
            <StartDate>@[START_DATE]@</StartDate>
          </ValidityPeriod>
        </DealTerms>
      </Deal>
      <Deal>
        <DealTerms>
          <CommercialModelType>RightsClaimModel</CommercialModelType>
          <Usage>
            <UseType>UserMakeAvailableUserProvided</UseType>
          </Usage>
          <TerritoryCode>@[TERRITORY_CODE]@</TerritoryCode>
          <ValidityPeriod/>
          <RightsClaimPolicy>
            <RightsClaimPolicyType>Monetize</RightsClaimPolicyType>
          </RightsClaimPolicy>
        </DealTerms>
      </Deal>
      <EffectiveDate>@[START_DATE]@</EffectiveDate>
    </ReleaseDeal>
		#END_LOOP_RELEASE_DEAL#
  </DealList>
</ern:NewReleaseMessage>